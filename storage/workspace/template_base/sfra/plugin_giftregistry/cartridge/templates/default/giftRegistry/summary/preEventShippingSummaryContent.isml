<div class="row">
    <span class="address-tittle-summary">${pdict.giftRegistryModel.preEventShippingAddress.name || ''}</span>
</div>

<div class="row">
    <span class="fname">${pdict.giftRegistryModel.preEventShippingAddress.firstName || ''}</span>
    <span class="lname">${pdict.giftRegistryModel.preEventShippingAddress.lastName || ''}</span>
</div>

<div class="row">
    <span class="summary-addr-1">${pdict.giftRegistryModel.preEventShippingAddress.address1 || ''}</span>
</div>

<isif condition="${pdict.giftRegistryModel.preEventShippingAddress.address2}">
    <div class="row">
        <span class="summary-addr-2">${pdict.giftRegistryModel.preEventShippingAddress.address2 || ''}</span>
    </div>
</isif>

<div class="row">
    <span class="city">${pdict.giftRegistryModel.preEventShippingAddress.city || ''}</span>
    <span class="state">${pdict.giftRegistryModel.preEventShippingAddress.stateCode || ''}</span>
    <span class="zip">${pdict.giftRegistryModel.preEventShippingAddress.postalCode || ''}</span>
</div>

<div class="row">
    <span class="phone">${pdict.giftRegistryModel.preEventShippingAddress.phone || ''}</span>
</div>
